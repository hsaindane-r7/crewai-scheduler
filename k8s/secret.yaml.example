# Copy this file to secret.yaml and fill in your actual API key.
# DO NOT commit secret.yaml to version control!
#
# To create from this template:
#   cp k8s/secret.yaml.example k8s/secret.yaml
#   # Edit secret.yaml and replace YOUR_GROQ_API_KEY_HERE with your actual key
#
# Or create directly with kubectl:
#   kubectl create secret generic groq-api-key \
#     --namespace=crewai-agent \
#     --from-literal=GROQ_API_KEY=your_actual_key_here

apiVersion: v1
kind: Secret
metadata:
  name: groq-api-key
  namespace: crewai-agent
type: Opaque
stringData:
  GROQ_API_KEY: "YOUR_GROQ_API_KEY_HERE"

